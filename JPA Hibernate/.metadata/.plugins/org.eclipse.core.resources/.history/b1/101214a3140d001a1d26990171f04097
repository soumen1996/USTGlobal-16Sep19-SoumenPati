package com.ustglobal.jspwithhibernateapp.manytomany;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Entity
@Table
@Data
public class Course {
	
	@Id
	@Column
	private int cid;
	private String cname;
}
