package com.ustglobal.jspwithhibernateapp.onetoone;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Entity
@Table
@Data
public class Person {
	
	@Id
	@Column
	private int pid;
	
	@Column
	private String name;
	
	

}
